function checkPrime(number){
	let factorial=0;
	for(let i=1; i<=number/2; i++){
		if(number %i == 0){
			factorial++;
		}
	}
	
if(factorial==2){
	return true;
}
return false;

}

let number =11;

let ans =checkPrime(number) ;
if(ans == true){
	console.log("prime");
}else{
	console.log("Not a Prime");
}